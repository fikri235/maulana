#include<iostream>
using namespace std;
int main (){
  int a=1, b=19, n; // GANJIL //
  cout << "Program menghitung 10 deret bilangan genap : " << endl;
  cout << "\n";
  for(a=a; a<=b; a+=2){
    cout << a;
    if(a<b){
      cout << " + ";
    }
  }
  cout << " = ";   //
  n=(b+1)/2;      //  RUMUS nya DISINI //
  n=n*n;         //
  cout << n;
  getchar();
}
